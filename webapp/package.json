      "name": "TSOLEGNAGBO Kossi",
      "email": "kossi.tsolegnagbo@gmail.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/KossiPascal/atlas-kanban"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --hmr",
    "startNoHmr": "ng serve --no-hmr",
    "start-ssl": "ng serve --ssl=true",
    "clean": "npm cache clean --force",
    "serve": "npm run clean && ng serve --configuration production",
    "build": "rm -rf ./.angular && rm -rf ../views && npm run clean && ng build --service-worker --optimization --configuration production --aot --output-hashing=all --named-chunks --stats-json",
    "simple-build": "ng build --service-worker --optimization --configuration production --aot --output-hashing=all --named-chunks --stats-json",
    "compile": "ngc",
    "watch": "ng build --watch --configuration development",
    "coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "test": "ng test",
    "precache": "sw-precache --verbose --config=sw-precache-config.js"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.2",
